Cell[BoxData[
 RowBox[{
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"byte1Seed", "=", 
       RowBox[{"{", 
        RowBox[{"1", ",", "4"}], "}"}]}], ",", "byte1Seq", ",", "twinPrimes", 
      ",", "isPrime", ",", "bbpDelta", ",", 
      RowBox[{"limit", "=", "5000"}], ",", 
      RowBox[{"n", "=", "3"}], ",", 
      RowBox[{"hTarget", "=", "0.35"}], ",", 
      RowBox[{"collapsePoints", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"hScores", "=", 
       RowBox[{"{", "}"}]}], ",", "hFunctionBase2"}], "}"}], ",", 
    RowBox[{"(*", 
     RowBox[{"Byte1", " ", 
      RowBox[{"sequence", ":", 
       RowBox[{
        RowBox[{"extended", " ", "recursion", " ", "using", " ", "binary"}], 
        "-", 
        RowBox[{"length", " ", "deltas"}]}]}]}], "*)"}], 
    RowBox[{
     RowBox[{"byte1Seq", "=", 
      RowBox[{
       RowBox[{"NestList", "[", 
        RowBox[{
         RowBox[{"Function", "[", 
          RowBox[{
           RowBox[{"{", "pair", "}"}], ",", 
           RowBox[{"Module", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"diff", "=", 
                RowBox[{"Abs", "[", 
                 RowBox[{
                  RowBox[{"pair", "[", 
                   RowBox[{"[", 
                    RowBox[{"-", "1"}], "]"}], "]"}], "-", 
                  RowBox[{"pair", "[", 
                   RowBox[{"[", 
                    RowBox[{"-", "2"}], "]"}], "]"}]}], "]"}]}], ",", "len"}],
               "}"}], ",", 
             RowBox[{
              RowBox[{"len", "=", 
               RowBox[{"IntegerLength", "[", 
                RowBox[{"diff", ",", "2"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"Append", "[", 
               RowBox[{"pair", ",", "len"}], "]"}]}]}], "]"}]}], "]"}], ",", 
         "byte1Seed", ",", "300"}], "]"}], "[", 
       RowBox[{"[", 
        RowBox[{"-", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Simple", " ", "primality", " ", "check"}], "*)"}], 
     RowBox[{
      RowBox[{"isPrime", "[", "n_", "]"}], ":=", 
      RowBox[{"PrimeQ", "[", "n", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"BBP", "-", 
       RowBox[{
       "modulated", " ", "twin", " ", "prime", " ", "step", " ", 
        "function"}]}], "*)"}], 
     RowBox[{
      RowBox[{"bbpDelta", "[", "n_", "]"}], ":=", 
      RowBox[{
       RowBox[{"Floor", "[", 
        RowBox[{"Total", "[", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"16", "^", 
             RowBox[{"(", 
              RowBox[{"1", "-", "k"}], ")"}]}], "/", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"8", " ", "k"}], "+", 
              RowBox[{"Mod", "[", 
               RowBox[{"n", ",", "7"}], "]"}], "+", "1"}], ")"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"k", ",", "1", ",", "4"}], "}"}]}], "]"}], "]"}], "]"}], 
       "+", "1"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Generate", " ", "twin", " ", "primes", " ", "with", " ", "BBP", " ", 
       "skips"}], "*)"}], 
     RowBox[{"twinPrimes", "=", 
      RowBox[{
       RowBox[{"Reap", "[", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"n", "<", "limit"}], ",", 
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"isPrime", "[", "n", "]"}], "&&", 
              RowBox[{"isPrime", "[", 
               RowBox[{"n", "+", "2"}], "]"}]}], ",", 
             RowBox[{"Sow", "[", 
              RowBox[{"{", 
               RowBox[{"n", ",", 
                RowBox[{"n", "+", "2"}]}], "}"}], "]"}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"n", "+=", 
            RowBox[{"bbpDelta", "[", "n", "]"}]}], ";"}]}], "]"}], "]"}], "[", 
       RowBox[{"[", 
        RowBox[{"2", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Define", " ", "harmonic", " ", "match", " ", "score", " ", "in", " ", 
        "base"}], "-", "2"}], "*)"}], 
     RowBox[{
      RowBox[{"hFunctionBase2", "[", "tp_", "]"}], ":=", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"digits", ",", "overlap", ",", "match"}], "}"}], ",", 
        RowBox[{
         RowBox[{"digits", "=", 
          RowBox[{"IntegerDigits", "[", 
           RowBox[{
            RowBox[{"tp", "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", "2"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"overlap", "=", 
          RowBox[{"Take", "[", 
           RowBox[{"byte1Seq", ",", 
            RowBox[{"Length", "[", "digits", "]"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"match", "=", 
          RowBox[{"Count", "[", 
           RowBox[{
            RowBox[{"MapThread", "[", 
             RowBox[{"Equal", ",", 
              RowBox[{"{", 
               RowBox[{"digits", ",", "overlap"}], "}"}]}], "]"}], ",", 
            "True"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"N", "[", 
          RowBox[{"match", "/", 
           RowBox[{"Length", "[", "digits", "]"}]}], "]"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Score", " ", "and", " ", "detect", " ", "collapse", " ", "zones"}], 
      "*)"}], 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"h", "=", 
           RowBox[{"hFunctionBase2", "[", "pair", "]"}]}], "}"}], ",", 
         RowBox[{
          RowBox[{"AppendTo", "[", 
           RowBox[{"hScores", ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Mean", "[", "pair", "]"}], ",", "h"}], "}"}]}], "]"}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"h", "-", "hTarget"}], "]"}], "<", "0.15"}], ",", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"collapsePoints", ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"Mean", "[", "pair", "]"}], ",", "1"}], "}"}]}], 
             "]"}]}], "]"}], ";"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"pair", ",", "twinPrimes"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Overlay", " ", "all", " ", "scores", " ", "and", " ", "collapse", " ", 
       "points"}], "*)"}], 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"ListPlot", "[", 
