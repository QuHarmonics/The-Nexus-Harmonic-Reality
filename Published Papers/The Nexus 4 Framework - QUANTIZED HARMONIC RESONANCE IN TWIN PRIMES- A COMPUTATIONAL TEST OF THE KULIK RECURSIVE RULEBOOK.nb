         RowBox[{"PlotStyle", "->", "Blue"}], ",", 
         RowBox[{"AxesLabel", "->", 
          RowBox[{"{", 
           RowBox[{
           "\"\<Twin Prime Midpoint\>\"", ",", "\"\<H(t) Match Score\>\""}], 
           "}"}]}], ",", 
         RowBox[{"PlotLegends", "->", 
          RowBox[{"{", "\"\<H(t) Score\>\"", "}"}]}]}], "]"}], ",", 
       RowBox[{"ListPlot", "[", 
        RowBox[{"collapsePoints", ",", 
         RowBox[{"PlotStyle", "->", 
          RowBox[{"{", 
           RowBox[{"Red", ",", 
            RowBox[{"PointSize", "[", "Medium", "]"}]}], "}"}]}], ",", 
         RowBox[{"PlotLegends", "->", 
